{
  "stryker-config": {
    "reporters": [
      "progress",
      "html"
    ],
    "project": "Application.csproj",
    "report-file-name": "mutation-tests-application-result",
    "thresholds": {
      "high": 90,
      "low": 80,
      "break": 50
    },
    "ignore-mutations": [
      "linq.First",
      "linq.Sum"
    ],
    "mutate": [
      "!**/*ApplicationDependencyInjection.cs"
    ]
  }
}